{
  "compilerOptions": {
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "module": "commonjs",
    "target": "es6",
    "downlevelIteration": true,
    "lib": [ "es2015" ],
    "outDir": "dist",
    "strict": true,
    "strictPropertyInitialization": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "Node"
  },
}
